<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.controllers.Controller">
    <top>
        <HBox>
            <Button onAction="#openFile" text="Open File">
                <HBox.margin>
                    <Insets/>
                </HBox.margin>
            </Button>
            <Button fx:id="settingsButton" onAction="#openSettings">
                <HBox.margin>
                    <Insets/>
                </HBox.margin>
            </Button>
        </HBox>
    </top>
    <center>
        <TabPane fx:id="tabPane"/>
    </center>
</BorderPane>
